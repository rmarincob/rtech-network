{
    "name": "mediledger-patient-network",
    "description": "Connection profile for patient in mediledger network",
    "version": "1.0.0",
    "client": {
        "tlsEnable": true,
        "enableAuthentication": false,
        "organization": "patient",
        "adminCredential": {
            "id": "admin",
            "password": "password"
        },
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                },
                "orderer": "300"
            }
        }
    },
    "channels": {
        "mediledger": {
            "peers": {
                "peer0.doctor.rtech.com": {},
                "peer1.doctor.rtech.com": {},
                "peer0.patient.rtech.com": {},
                "peer1.patient.rtech.com": {}
            },
            "connection": {
                "timeout": {
                    "peer": {
                        "endorser": "300",
                        "eventHub": "300",
                        "eventReg": "300"
                    }
                }
            }
        }
    },
    "organizations": {
        "patient": {
            "mspid": "patientMSP",
            "adminPrivateKey": {
				"path": "../blockchain/crypto-config/peerOrganizations/patient.rtech.com/users/Admin@patient.rtech.com/msp/keystore/priv_sk"
			},
            "signedCert": {
				"path": "../blockchain/crypto-config/peerOrganizations/patient.rtech.com/users/Admin@patient.rtech.com/msp/signcerts/Admin@patient.rtech.com-cert.pem"
			},
            "peers": [
                "peer0.doctor.rtech.com",
                "peer1.doctor.rtech.com",
                "peer0.patient.rtech.com",
                "peer1.patient.rtech.com"
            ],
            "certificateAuthorities": [
                "ca.patient.rtech.com"
            ]
        }
    },
    "peers": {
        "peer0.doctor.rtech.com": {
            "url": "grpcs://localhost:7010",
            "tlsCACerts": {
                "path": "../blockchain/crypto-config/peerOrganizations/doctor.rtech.com/peers/peer0.doctor.rtech.com/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.doctor.rtech.com"
            }
        },
        "peer1.doctor.rtech.com": {
            "url": "grpcs://localhost:7011",
            "tlsCACerts": {
                "path": "../blockchain/crypto-config/peerOrganizations/doctor.rtech.com/peers/peer1.doctor.rtech.com/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.doctor.rtech.com"
            }
        },
        "peer0.patient.rtech.com": {
            "url": "grpcs://localhost:7012",
            "tlsCACerts": {
                "path": "../blockchain/crypto-config/peerOrganizations/patient.rtech.com/peers/peer0.patient.rtech.com/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.patient.rtech.com"
            }
        },
        "peer1.patient.rtech.com": {
            "url": "grpcs://localhost:7013",
            "tlsCACerts": {
                "path": "../blockchain/crypto-config/peerOrganizations/patient.rtech.com/peers/peer1.patient.rtech.com/tls/ca.crt"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.patient.rtech.com"
            }
        }
    },
    "certificateAuthorities": {
        "ca.patient.rtech.com": {
            "url": "http://localhost:7081",
            "caName": "ca.patient.rtech.com",
            "tlsCACerts": {
                "path": "../blockchain/crypto-config/peerOrganizations/patient.rtech.com/ca/ca.patient.rtech.com-cert.pem"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}